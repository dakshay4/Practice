<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jwt.login.Validate" table="REGISTER">
		<id column="ID" name="id" type="int">
		<generator class="increment"/>
		</id> 
		<property column="USERID" name="userid" type="string" not-null="true" />
		<property column="EMAIL" name="email" type="string" />
		<property column="PASS" name="pass" type="string" />
		<property column="MOBILE_NO" name="mobile" type="long" />
		<property column="PROFILE" name="profile" type="int" />
	</class>
</hibernate-mapping>